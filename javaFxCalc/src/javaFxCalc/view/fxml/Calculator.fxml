<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javaFxCalc.controller.CalculatorController">
   <top>
		<AnchorPane prefHeight="25.0" BorderPane.alignment="CENTER">
   			<children>
   				<TextField fx:id="resultText" AnchorPane.leftAnchor="3.0" AnchorPane.rightAnchor="3.0" BorderPane.alignment="CENTER" />
   			</children>
		</AnchorPane>
   </top>
   <center>
      <GridPane BorderPane.alignment="CENTER">
        <columnConstraints>
          	<ColumnConstraints hgrow="ALWAYS" minWidth="10.0" />
          	<ColumnConstraints hgrow="ALWAYS" minWidth="10.0" />
			<ColumnConstraints hgrow="ALWAYS" minWidth="10.0" />
        </columnConstraints>
        <rowConstraints>
          	<RowConstraints minHeight="10.0" vgrow="ALWAYS" />
          	<RowConstraints minHeight="10.0" vgrow="ALWAYS" />
          	<RowConstraints minHeight="10.0" vgrow="ALWAYS" />
            <RowConstraints minHeight="10.0" vgrow="ALWAYS" />
        </rowConstraints>
         <padding>
            <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
         </padding>
         <children>
            <AnchorPane BorderPane.alignment="CENTER">
               <children>
               	<Button fx:id="btn0" onAction="#addNumber" mnemonicParsing="false" text="0" AnchorPane.bottomAnchor="3.0" AnchorPane.leftAnchor="3.0" AnchorPane.rightAnchor="3.0" AnchorPane.topAnchor="3.0" />
               </children>
            </AnchorPane>
            <AnchorPane BorderPane.alignment="CENTER" GridPane.columnIndex="1">
               <children>
               	<Button fx:id="btn1" onAction="#addNumber" mnemonicParsing="false" text="1" AnchorPane.bottomAnchor="3.0" AnchorPane.leftAnchor="3.0" AnchorPane.rightAnchor="3.0" AnchorPane.topAnchor="3.0" GridPane.columnIndex="1" />
               </children>
            </AnchorPane>
            <AnchorPane BorderPane.alignment="CENTER" GridPane.columnIndex="2">
               <children>
               	<Button fx:id="btn2" onAction="#addNumber" mnemonicParsing="false" text="2" AnchorPane.bottomAnchor="3.0" AnchorPane.leftAnchor="3.0" AnchorPane.rightAnchor="3.0" AnchorPane.topAnchor="3.0" GridPane.columnIndex="1" />
               </children>
            </AnchorPane>
            <AnchorPane BorderPane.alignment="CENTER" GridPane.rowIndex="1">
               <children>
               	<Button fx:id="btn3" onAction="#addNumber" mnemonicParsing="false" text="3" AnchorPane.bottomAnchor="3.0" AnchorPane.leftAnchor="3.0" AnchorPane.rightAnchor="3.0" AnchorPane.topAnchor="3.0" GridPane.columnIndex="1" />
               </children>
            </AnchorPane>
            <AnchorPane BorderPane.alignment="CENTER" GridPane.columnIndex="1" GridPane.rowIndex="1">
               <children>
               	<Button fx:id="btn4" onAction="#addNumber" mnemonicParsing="false" text="4" AnchorPane.bottomAnchor="3.0" AnchorPane.leftAnchor="3.0" AnchorPane.rightAnchor="3.0" AnchorPane.topAnchor="3.0" GridPane.rowIndex="2" />
               </children>
            </AnchorPane>
			<AnchorPane BorderPane.alignment="CENTER" GridPane.columnIndex="2" GridPane.rowIndex="1">
               <children>
               	<Button fx:id="btn5" onAction="#addNumber" mnemonicParsing="false" text="5" AnchorPane.bottomAnchor="3.0" AnchorPane.leftAnchor="3.0" AnchorPane.rightAnchor="3.0" AnchorPane.topAnchor="3.0" GridPane.rowIndex="2" />
               </children>
            </AnchorPane>
            <AnchorPane BorderPane.alignment="CENTER" GridPane.rowIndex="2">
               <children>
               	<Button fx:id="btn6" onAction="#addNumber" mnemonicParsing="false" text="6" AnchorPane.bottomAnchor="3.0" AnchorPane.leftAnchor="3.0" AnchorPane.rightAnchor="3.0" AnchorPane.topAnchor="3.0" GridPane.rowIndex="2" />
               </children>
            </AnchorPane>
            <AnchorPane BorderPane.alignment="CENTER" GridPane.columnIndex="1" GridPane.rowIndex="2">
               <children>
               	<Button fx:id="btn7" onAction="#addNumber" mnemonicParsing="false" text="7" AnchorPane.bottomAnchor="3.0" AnchorPane.leftAnchor="3.0" AnchorPane.rightAnchor="3.0" AnchorPane.topAnchor="3.0" GridPane.rowIndex="2" />
               </children>
            </AnchorPane>
			<AnchorPane BorderPane.alignment="CENTER" GridPane.columnIndex="2" GridPane.rowIndex="2">
               <children>
               	<Button fx:id="btn8" onAction="#addNumber" mnemonicParsing="false" text="8" AnchorPane.bottomAnchor="3.0" AnchorPane.leftAnchor="3.0" AnchorPane.rightAnchor="3.0" AnchorPane.topAnchor="3.0" GridPane.rowIndex="2" />
               </children>
            </AnchorPane>
            <AnchorPane BorderPane.alignment="CENTER" GridPane.rowIndex="3">
               <children>
               	<Button fx:id="btn9" onAction="#addNumber" mnemonicParsing="false" text="9" AnchorPane.bottomAnchor="3.0" AnchorPane.leftAnchor="3.0" AnchorPane.rightAnchor="3.0" AnchorPane.topAnchor="3.0" GridPane.rowIndex="2" />
               </children>
            </AnchorPane>
         </children>
      </GridPane>
   </center>
	<right>
		<GridPane BorderPane.alignment="CENTER">
        	<columnConstraints>
          		<ColumnConstraints hgrow="ALWAYS" minWidth="40.0" />
        	</columnConstraints>
        	<rowConstraints>
          		<RowConstraints minHeight="10.0" vgrow="ALWAYS" />
          		<RowConstraints minHeight="10.0" vgrow="ALWAYS" />
          		<RowConstraints minHeight="10.0" vgrow="ALWAYS" />
            	<RowConstraints minHeight="10.0" vgrow="ALWAYS" />
            	<RowConstraints minHeight="10.0" vgrow="ALWAYS" />
        	</rowConstraints>
         	<padding>
            	<Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
         	</padding>
         	<children>
         		<AnchorPane BorderPane.alignment="CENTER" GridPane.rowIndex="0">
	   				<children>
	      				<Button fx:id="btnReset" onAction="#resetTextField" mnemonicParsing="false" text="C" AnchorPane.bottomAnchor="3.0" AnchorPane.leftAnchor="3.0" AnchorPane.rightAnchor="3.0" AnchorPane.topAnchor="3.0" />
	      			</children>
	   			</AnchorPane>
				<AnchorPane BorderPane.alignment="CENTER" GridPane.rowIndex="1">
	   				<children>
	      				<Button fx:id="btnPlus" onAction="#addOperation" mnemonicParsing="false" text="+" AnchorPane.bottomAnchor="3.0" AnchorPane.leftAnchor="3.0" AnchorPane.rightAnchor="3.0" AnchorPane.topAnchor="3.0" />
	      			</children>
	   			</AnchorPane>
				<AnchorPane BorderPane.alignment="CENTER" GridPane.rowIndex="2">
	   				<children>
	      				<Button fx:id="btnMinus" onAction="#addOperation" mnemonicParsing="false" text="-" AnchorPane.bottomAnchor="3.0" AnchorPane.leftAnchor="3.0" AnchorPane.rightAnchor="3.0" AnchorPane.topAnchor="3.0" />
	      			</children>
	   			</AnchorPane>
	   			<AnchorPane BorderPane.alignment="CENTER" GridPane.rowIndex="3">
	   				<children>
	      				<Button fx:id="btnMulty" onAction="#addOperation" mnemonicParsing="false" text="*" AnchorPane.bottomAnchor="3.0" AnchorPane.leftAnchor="3.0" AnchorPane.rightAnchor="3.0" AnchorPane.topAnchor="3.0" />
	      			</children>
	   			</AnchorPane>
	   			<AnchorPane BorderPane.alignment="CENTER" GridPane.rowIndex="4">
	   				<children>
	      				<Button fx:id="btnDivide" onAction="#addOperation" mnemonicParsing="false" text="/" AnchorPane.bottomAnchor="3.0" AnchorPane.leftAnchor="3.0" AnchorPane.rightAnchor="3.0" AnchorPane.topAnchor="3.0" />
	      			</children>
	   			</AnchorPane>
         	</children>
		</GridPane>
   </right>
   <bottom>
   	<AnchorPane BorderPane.alignment="CENTER">
   		<children>
        	<Button fx:id="btnSum" onAction="#summValues" mnemonicParsing="false" text="=" AnchorPane.bottomAnchor="3.0" AnchorPane.leftAnchor="3.0" AnchorPane.rightAnchor="3.0" />
        </children>
   	</AnchorPane>
   </bottom>
</BorderPane>
